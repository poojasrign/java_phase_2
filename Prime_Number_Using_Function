import java.util.*;
public class Main
{

	static boolean prime (int a) {
		for(int i = 2 ; i <(a/2)+1; i++) {
			if(a%i==0)
				return false;
		}
		return true;
	}
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		ArrayList<Integer> prime_list = new ArrayList<>();
		for(int i = 2 ; i <= n; i++) {
			if(prime(i))
				prime_list.add(i);
		}
		System.out.println(prime_list);
	}

}

Output:
5
[2, 3, 5]
