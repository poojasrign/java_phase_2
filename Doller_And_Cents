import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
	    Scanner sc=new Scanner(System.in);
	    System.out.println("Enter a Doller 1");
	    int d1=sc.nextInt();
	    System.out.println("Enter a Doller 2");
	    int c1=sc.nextInt();
	    System.out.println("Enter a cents 1");
	    int d2=sc.nextInt();
	    System.out.println("Enter a cents 2");
	    int c2=sc.nextInt();
	    int sumD=d1+d2;
	    int sumC=c1+c2;
	    int doll= sumC/100;
	    int cen=sumC%100;
	    int totalDoll = sumD+doll;
	    int totalcent = cen;
		System.out.println("\n Total Doller" + totalDoll);
		System.out.println("\n Total Cents" + totalcent);
	}
}

Output:
Enter a Doller 1
5
Enter a Doller 2
5
Enter a cents 1
100
Enter a cents 2
100

 Total Doller 106

 Total Cents 5
