import java.util.*;
public class Main
{
	public static void main(String[] args) {
		Queue<String> queue = new LinkedList<>();
        queue.offer("First");
        queue.offer("Second");
        queue.offer("Third");
        System.out.println("Queue\n"+queue);
        System.out.println("Peek\n"+queue.peek());
        System.out.println("Queue After Peek\n"+queue);
        System.out.println("Pool\n"+queue.poll());
        System.out.println("Queue After Peek\n"+queue);
        System.out.println(queue);
	}
}

Output:
Queue
[First, Second, Third]
Peek
First
Queue After Peek
[First, Second, Third]
Pool
First
Queue After Peek
[Second, Third]
[Second, Third]
