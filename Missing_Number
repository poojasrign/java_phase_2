public class Main
{
	public static void main(String[] args) {
	    
        int[] arr = {1,2,4,5};
        int n=arr.length+1;
        int sumforall =(n* (n + 1) )/2;
        int array_sum = 0;
        for(int i: arr){
            array_sum += i;
        }
        System.out.println(sumforall-array_sum);
	}

}

output:
3
