import java.util.*;
public class Main
{
    static int sum(int arr[],int index){
        if(index==0)
          return arr[0];
        return arr[index]+sum(arr,index-1);    
        
    }
	public static void main(String[] args) {
	    Scanner sc= new Scanner(System.in);
	    int n=sc.nextInt();
	    int arr[] = new int[n];
	    for(int i=0;i<n;i++){
	        arr[i]=sc.nextInt();
	    }
		System.out.println(sum(arr,arr.length-1));
	}
}

Output:
5
1 2 3 4 5
15
